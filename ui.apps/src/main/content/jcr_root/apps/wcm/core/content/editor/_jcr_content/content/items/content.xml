<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:vlt="http://www.day.com/jcr/vault/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="sling:OrderedFolder">
    <header
        jcr:primaryType="nt:unstructured">
        <items jcr:primaryType="nt:unstructured">
            <headerbar
                jcr:primaryType="nt:unstructured">
                <items jcr:primaryType="nt:unstructured">
                    <pageinfopopover
                        jcr:primaryType="nt:unstructured">
                        <items jcr:primaryType="nt:unstructured">
                            <list
                                jcr:primaryType="nt:unstructured">
                                <items jcr:primaryType="nt:unstructured">
                                    <backpack
                                            granite:class="js-create-package-activator"
                                            granite:title="Pack by Backpack"
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/button"
                                            icon="backpack-sites"
                                            text="Create package">
                                        <granite:data
                                                jcr:primaryType="nt:unstructured"
                                                path="${requestPathInfo.suffix}"/>
                                    </backpack>
                                </items>
                            </list>
                        </items>
                    </pageinfopopover>
                </items>
            </headerbar>
        </items>
    </header>
</jcr:root>